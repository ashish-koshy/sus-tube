{
    "name": "sus-tube",
    "version": "0.0.1",
    "scripts": {
        "ng": "ng",
        "e2e": "ng e2e",
        "ionic": "ionic",
        "eslint": "eslint",
        "pretty-quick": "pretty-quick",
        "start": "ng serve -o",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test --watch=false --browsers ChromeHeadless",
        "prepare": "husky install",
        "lint-staged": "lint-staged",
        "test-pwa": "ng build --configuration production && npx http-server www",
        "generate-app-shell": "ng run app:app-shell:production",
        "dev:ssr": "ng run app:serve-ssr",
        "serve:ssr": "node dist/app/server/main.js",
        "build:ssr": "ng build && ng run app:server",
        "prerender": "ng run app:prerender",
        "ionic-build": "rimraf dist && npm run ionic build",
        "ionic-aot": "npm run ionic-build -- --aot",
        "ionic-start": "npm run ionic-build -- --watch",
        "android-ide": "npm run ionic-aot && npx cap sync android && npx cap open android",
        "android-device": "ionic capacitor run android -l --external --host=localhost --port=4200 --livereload-port=4200",
        "android-start": "npm-run-all -p ionic-start android-device",
        "ios-ide": "npm run ionic-aot && npx cap sync ios && npx cap open ios",
        "ios-device": "ionic capacitor run ios -l --external --host=localhost --port=4200 --livereload-port=4200",
        "ios-start": "npm-run-all -p ionic-start ios-device",
        "ionic-prod": "npm run ionic-build -- --configuration production"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ashish-koshy/sus-tube.git"
    },
    "author": "Ashish Koshy",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ashish-koshy/sus-tube/issues"
    },
    "homepage": "https://github.com/ashish-koshy/sus-tube#readme",
    "private": true,
    "dependencies": {
        "@angular/animations": "^14.2.4",
        "@angular/cdk": "^14.2.3",
        "@angular/common": "^14.0.0",
        "@angular/core": "^14.0.0",
        "@angular/forms": "^14.0.0",
        "@angular/localize": "^14.2.4",
        "@angular/material": "^14.2.3",
        "@angular/platform-browser": "^14.0.0",
        "@angular/platform-browser-dynamic": "^14.0.0",
        "@angular/platform-server": "^14.0.0",
        "@angular/router": "^14.0.0",
        "@angular/service-worker": "^14.0.0",
        "@capacitor/android": "4.3.0",
        "@capacitor/app": "4.0.1",
        "@capacitor/core": "4.3.0",
        "@capacitor/haptics": "4.0.1",
        "@capacitor/ios": "^4.3.0",
        "@capacitor/keyboard": "4.0.1",
        "@capacitor/status-bar": "4.0.1",
        "@ionic/angular": "^6.1.9",
        "@ng-bootstrap/ng-bootstrap": "^13.0.0",
        "@nguniversal/express-engine": "^14.2.0",
        "@popperjs/core": "^2.11.6",
        "bootstrap": "^5.2.2",
        "express": "^4.15.2",
        "ionicons": "^6.0.3",
        "rxjs": "~6.6.0",
        "tslib": "^2.2.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^14.0.0",
        "@angular-devkit/core": "^14.2.4",
        "@angular-devkit/schematics": "^14.2.4",
        "@angular-eslint/builder": "^14.1.2",
        "@angular-eslint/eslint-plugin": "^14.1.2",
        "@angular-eslint/eslint-plugin-template": "^14.1.2",
        "@angular-eslint/schematics": "^14.1.2",
        "@angular-eslint/template-parser": "^14.1.2",
        "@angular/cli": "^14.0.0",
        "@angular/compiler": "^14.2.4",
        "@angular/compiler-cli": "^14.0.0",
        "@angular/language-service": "^14.0.0",
        "@ashish-koshy/ng-schematics": "^13.0.2",
        "@capacitor/cli": "4.3.0",
        "@ionic/angular-toolkit": "^6.0.0",
        "@ionic/cli": "^6.20.3",
        "@nguniversal/builders": "^14.2.0",
        "@schematics/angular": "^14.2.4",
        "@types/express": "^4.17.0",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "5.3.0",
        "@typescript-eslint/parser": "5.3.0",
        "eslint": "^7.6.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jsdoc": "30.7.6",
        "eslint-plugin-prefer-arrow": "1.2.2",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.1",
        "jasmine-core": "~3.8.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.3.2",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "lint-staged": "^13.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "typescript": "~4.7.3"
    },
    "lint-staged": {
        "*.{js,ts}": [
            "npm run eslint -- --fix"
        ],
        "*.{js,ts,html,css,scss,json}": [
            "npm run pretty-quick -- --staged"
        ]
    },
    "description": "A Sus YouTube"
}
