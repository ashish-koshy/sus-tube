{
    "name": "sus-tube",
    "version": "0.0.1",
    "scripts": {
        "ng": "ng",
        "e2e": "ng e2e",
        "ionic": "ionic",
        "eslint": "eslint",
        "prettier": "prettier",
        "prepare": "husky install",
        "lint-staged": "lint-staged",
        "pretty-quick": "pretty-quick",
        "start": "npm run ionic serve -- -o --port=4200",
        "ionic-build": "rimraf dist && npm run ionic capacitor build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test --watch=false --browsers ChromeHeadless",
        "android-build": "npm run ionic-build android && npx cap sync android",
        "android-ide": "npm run android-build && npx cap open android",
        "android-live": "npm run android-build && npm run ionic cap run android -- -l --external",
        "android-release": "npm run ionic-build android -- -- --prod",
        "ios-build": "npm run ionic-build ios && npx cap sync ios",
        "ios-ide": "npm run ios-build && npx cap open ios",
        "ios-live": "npm run ios-build && npm run ionic cap run ios -- -l --external",
        "ios-release": "npm run ionic-build ios -- -- --prod",
        "web-release": "npm run ionic build -- --prod",
        "test-pwa": "npm run web-release && npx http-server dist/app/browser"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ashish-koshy/sus-tube.git"
    },
    "author": "Ashish Koshy",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ashish-koshy/sus-tube/issues"
    },
    "homepage": "https://github.com/ashish-koshy/sus-tube#readme",
    "private": true,
    "dependencies": {
        "@angular/animations": "^14.2.4",
        "@angular/cdk": "^14.2.3",
        "@angular/common": "^14.0.0",
        "@angular/core": "^14.0.0",
        "@angular/forms": "^14.0.0",
        "@angular/localize": "^14.2.4",
        "@angular/material": "^14.2.3",
        "@angular/platform-browser": "^14.0.0",
        "@angular/platform-browser-dynamic": "^14.0.0",
        "@angular/platform-server": "^14.0.0",
        "@angular/router": "^14.0.0",
        "@angular/service-worker": "^14.0.0",
        "@capacitor/android": "4.3.0",
        "@capacitor/app": "4.0.1",
        "@capacitor/core": "4.3.0",
        "@capacitor/haptics": "4.0.1",
        "@capacitor/ios": "4.3.0",
        "@capacitor/keyboard": "4.0.1",
        "@capacitor/status-bar": "4.0.1",
        "@ionic/angular": "^6.1.9",
        "@ng-bootstrap/ng-bootstrap": "^13.0.0",
        "@nguniversal/express-engine": "^14.2.0",
        "@popperjs/core": "^2.11.6",
        "bootstrap": "^5.2.2",
        "express": "^4.15.2",
        "ionicons": "^6.0.3",
        "rxjs": "~6.6.0",
        "tslib": "^2.2.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^14.0.0",
        "@angular-devkit/core": "^14.2.4",
        "@angular-devkit/schematics": "^14.2.4",
        "@angular-eslint/builder": "^14.1.2",
        "@angular-eslint/eslint-plugin": "^14.1.2",
        "@angular-eslint/eslint-plugin-template": "^14.1.2",
        "@angular-eslint/schematics": "^14.1.2",
        "@angular-eslint/template-parser": "^14.1.2",
        "@angular/cli": "^14.0.0",
        "@angular/compiler": "^14.2.4",
        "@angular/compiler-cli": "^14.0.0",
        "@angular/language-service": "^14.0.0",
        "@ashish-koshy/ng-schematics": "^13.0.2",
        "@capacitor/cli": "4.3.0",
        "@ionic/angular-toolkit": "^6.0.0",
        "@ionic/cli": "^6.20.3",
        "@nguniversal/builders": "^14.2.0",
        "@schematics/angular": "^14.2.4",
        "@types/express": "^4.17.0",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "5.3.0",
        "@typescript-eslint/parser": "5.3.0",
        "eslint": "^7.6.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jsdoc": "30.7.6",
        "eslint-plugin-prefer-arrow": "1.2.2",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.1",
        "jasmine-core": "~3.8.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.3.2",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "lint-staged": "^13.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "typescript": "~4.7.3"
    },
    "lint-staged": {
        "*.{js,ts}": [
            "npm run eslint -- --fix"
        ],
        "*.{js,ts,html,css,scss,json}": [
            "npm run pretty-quick -- --staged"
        ]
    },
    "description": "A Sus YouTube"
}
